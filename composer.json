{
    "name": "codefog/contao-events_subscriptions",
    "description": "events_subscriptions extension for Contao Open Source CMS",
    "keywords": [
        "contao",
        "event",
        "events",
        "subscription",
        "subscriptions",
        "calendar"
    ],
    "type": "contao-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3",
        "contao/calendar-bundle": "^5.3",
        "codefog/contao-haste": "^5.2",
        "terminal42/notification_center": "^2.4",
        "league/csv": "^9.23",
        "phpoffice/phpspreadsheet": "^4.3",
        "symfony/mime": "^6.4 || ^7.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "terminal42/contao-build-tools": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Codefog\\EventsSubscriptionsBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "Codefog\\EventsSubscriptionsBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "terminal42/contao-build-tools": true,
            "php-http/discovery": true
        }
    }
}
